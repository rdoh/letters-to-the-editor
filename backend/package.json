{
  "name": "letters-to-the-editor-backend",
  "version": "0.0.1",
  "description": "Letters to the Editor",
  "scripts": {
    "start": "node ../bin/www",
    "migrate": "sequelize --config config/db.json db:migrate",
    "pretest": "NODE_ENV=test npm run migrate",
    "prestart": "npm run migrate",
    "test": "NODE_ENV=test mocha --check-leaks --timeout 10000 --compilers js:babel-core/register --recursive spec/"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "body-parser": "^1.15.0",
    "config": "^1.20.4",
    "csv-parse": "^1.1.0",
    "ejs": "^2.4.1",
    "express": "^4.13.4",
    "express-sanitized": "^0.5.1",
    "helmet": "^2.1.0",
    "node-neat": "^1.7.2",
    "node-sass-middleware": "^0.9.8",
    "node-uuid": "^1.4.7",
    "nodemailer": "^2.4.1",
    "nodemailer-mailgun-transport": "^1.0.2",
    "pg": "^4.5.5",
    "sequelize": "^3.23.2",
    "sequelize-cli": "^2.4.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "sinon": "^1.17.4",
    "supertest": "^1.2.0",
    "supertest-as-promised": "^3.1.0"
  }
}
